> pobs
function (x, na.last = "keep", ties.method = c("average", "first", 
    "random", "max", "min"), lower.tail = TRUE) 
{
    U <- apply(x, 2, rank, na.last = na.last, ties.method = ties.method)/(nrow(x) + 
        1)
    if (lower.tail) 
        U
    else 1 - U
}
